.underline {
  border-bottom: solid 1px #D1D1D1;
}
.underline-light {
  border-bottom: #E6E5F4;
}
.border-right {
  border-right: solid 1px #D1D1D1;
}
.filter {
  width: 100%;
  height: 100rpx;
}
.filter .selection {
  margin-top: 15rpx;
  height: 70rpx;
  line-height: 70rpx;
  width: 49.6%;
  display: inline-block;
  text-align: center;
  color: #FF9E9E;
}
.filter .selection .selector {
  width: 20rpx;
  height: 15rpx;
  float: right;
  margin-right: 100rpx;
  margin-top: 30rpx;
}
.filter .selection text {
  margin-left: 100rpx;
}
.form {
  width: 100%;
  border-top: solid 1px #D1D1D1;
  border-bottom: solid 1px #D1D1D1;
}
.form .row {
  width: 95%;
  float: right;
  height: 114rpx;
}
.form .row label {
  line-height: 114rpx;
  float: left;
}
.form .row input {
  height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row picker {
  height: 114rpx;
  line-height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row .info-divider {
  text-align: right;
  float: right;
  height: 114rpx;
  line-height: 114rpx;
  width: 40rpx;
  text-align: center;
}
.form .row .selector {
  width: 30rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.list {
  width: 100%;
  border-top: solid 1px #D1D1D1;
}
.list .row {
  width: 100%;
  float: right;
  height: 114rpx;
}
.list .row input {
  margin-left: 5%;
  height: 114rpx;
  text-align: left;
  float: left;
}
.list .row .delete-btn {
  width: 40rpx;
  height: 40rpx;
  margin-top: 37rpx;
  margin-right: 20rpx;
  float: right;
}
.list .row label {
  margin-left: 5%;
  line-height: 114rpx;
  float: left;
}
.list .row .selector {
  width: 100rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.logo {
  margin-top: 20rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 10px;
}
.bottom-row {
  height: 110rpx;
  line-height: 110rpx;
  width: 100%;
  border-top: solid 1px #D1D1D1;
  position: absolute;
  bottom: 0;
  font-size: 35rpx;
}
.bottom-row .info {
  float: left;
  width: 69%;
  height: 100%;
  text-indent: 50rpx;
}
.bottom-row .btn {
  float: right;
  width: 30%;
  height: 100%;
  background-color: #FF4343;
  text-align: center;
  color: white;
}
.std-button {
  width: 100%;
  margin-top: 30rpx;
}
.std-button .bind-user {
  background-color: #FF4343;
  color: white;
  width: 90%;
  height: 90rpx;
}
.edit-categories-main {
  align-items: right;
}
.edit-categories-main .save-edit-pos {
  position: absolute;
  bottom: 0;
  margin-bottom: 50rpx;
}
.edit-categories-main .edit-btn-wrapper {
  width: 100%;
  height: 114rpx;
  line-height: 114rpx;
  background-color: #F8F8F8;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.edit-categories-main .edit-btn-wrapper .second-wrapper {
  height: 114rpx;
  line-height: 114rpx;
  width: 120rpx;
  color: #FF4343;
}
.edit-categories-main .edit-btn-wrapper .second-wrapper .add-category-btn {
  margin-top: 30rpx;
  width: 50rpx;
  height: 50rpx;
  float: left;
}
.edit-categories-main .edit-btn-wrapper .second-wrapper .add-category-btn image {
  width: 45%;
  height: 45%;
  margin-bottom: 70%;
  margin-left: 30%;
}
.edit-categories-main .edit-btn-wrapper .second-wrapper text {
  float: right;
}

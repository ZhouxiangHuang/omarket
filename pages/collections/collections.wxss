.underline {
  border-bottom: solid 1px #D1D1D1;
}
.underline-light {
  border-bottom: #E6E5F4;
}
.border-right {
  border-right: solid 1px #D1D1D1;
}
.filter {
  width: 100%;
  height: 100rpx;
}
.filter .selection {
  margin-top: 15rpx;
  height: 70rpx;
  line-height: 70rpx;
  width: 49.6%;
  display: inline-block;
  text-align: center;
  color: #FF9E9E;
}
.filter .selection .selector {
  width: 20rpx;
  height: 15rpx;
  float: right;
  margin-right: 100rpx;
  margin-top: 30rpx;
}
.filter .selection text {
  margin-left: 100rpx;
}
.form {
  width: 100%;
  border-top: solid 1px #D1D1D1;
  border-bottom: solid 1px #D1D1D1;
}
.form .row {
  width: 95%;
  float: right;
  height: 114rpx;
}
.form .row label {
  line-height: 114rpx;
  float: left;
}
.form .row input {
  height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row picker {
  height: 114rpx;
  line-height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row .info-divider {
  text-align: right;
  float: right;
  height: 114rpx;
  line-height: 114rpx;
  width: 40rpx;
  text-align: center;
}
.form .row .selector {
  width: 30rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.list {
  width: 100%;
  border-top: solid 1px #D1D1D1;
}
.list .row {
  width: 100%;
  float: right;
  height: 114rpx;
}
.list .row input {
  margin-left: 5%;
  height: 114rpx;
  text-align: left;
  float: left;
}
.list .row .delete-btn {
  width: 100rpx;
  height: 114rpx;
  float: right;
}
.list .row label {
  margin-left: 5%;
  line-height: 114rpx;
  float: left;
}
.list .row .selector {
  width: 100rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.logo {
  margin-top: 20rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 10px;
}
.bottom-row {
  height: 110rpx;
  line-height: 110rpx;
  width: 100%;
  border-top: solid 1px #D1D1D1;
  position: absolute;
  bottom: 0;
  font-size: 35rpx;
}
.bottom-row .info {
  float: left;
  width: 69%;
  height: 100%;
  text-indent: 50rpx;
}
.bottom-row .btn {
  float: right;
  width: 30%;
  height: 100%;
  background-color: #FF4343;
  text-align: center;
  color: white;
}
.std-button {
  width: 100%;
  margin-top: 30rpx;
}
.std-button .bind-user {
  background-color: #FF4343;
  color: white;
  width: 90%;
  height: 90rpx;
}
.collections-main .product-list {
  border: solid 1px #D1D1D1;
  min-height: 300rpx;
  width: 100%;
}
.collections-main .product-list .title {
  height: 65rpx;
  width: 95%;
  float: right;
}
.collections-main .product-list .title .btn {
  color: #FF4343;
  float: left;
  width: 110rpx;
  margin-top: 10rpx;
  height: 45rpx;
  line-height: 45rpx;
  border: 1px solid #FF4343;
  text-indent: 50rpx;
  border-radius: 12px;
  font-size: 25rpx;
}
.collections-main .product-list .title .name {
  margin-left: 10rpx;
  float: left;
  width: 140rpx;
  height: 65rpx;
  line-height: 65rpx;
  font-size: 34rpx;
}
.collections-main .product-list .title .selector {
  width: 30rpx;
  height: 20rpx;
  margin-top: 22rpx;
  margin-right: 20rpx;
  float: right;
}
.collections-main .product-list .content {
  width: 95%;
  height: 235rpx;
  float: right;
}
.collections-main .product-list .content .left {
  float: left;
  width: 210rpx;
  height: 100%;
}
.collections-main .product-list .content .left image {
  border: 1px solid red;
  width: 210rpx;
  height: 210rpx;
  margin-top: 6%;
}
.collections-main .product-list .content .right {
  float: right;
  width: 460rpx;
  height: 100%;
}
.collections-main .product-list .content .right .name {
  margin-top: 35rpx;
  font-size: 27rpx;
}
.collections-main .product-list .content .right .price {
  margin-top: 15rpx;
  font-size: 27rpx;
}
.collections-main .product-list .content .right .code {
  margin-top: 15rpx;
  font-size: 20rpx;
}
.collections-main .product-list .content .right .description {
  font-size: 20rpx;
}
.collections-main .product-list .content .right .trash {
  width: 12%;
  position: absolute;
  top: 0;
  right: 0;
  margin-top: 90rpx;
}

.margin-space-top {
  margin-top: 30rpx;
}
.underline {
  border-bottom: solid 1px #D1D1D1;
}
.underline-light {
  border-bottom: #E6E5F4;
}
.underline-light-gray {
  border-bottom: solid 1px #E2E2E2;
}
.border-right {
  border-right: solid 1px #D1D1D1;
}
.filter {
  background-color: white;
  width: 100%;
  height: 90rpx;
}
.filter .selection {
  margin-top: 15rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 49.6%;
  display: inline-block;
  text-align: center;
  color: #FF9E9E;
}
.filter .selection .selector {
  width: 20rpx;
  height: 15rpx;
  float: right;
  margin-right: 100rpx;
  margin-top: 25rpx;
}
.filter .selection text {
  margin-left: 100rpx;
}
.form {
  width: 100%;
  border-top: solid 1px #D1D1D1;
  border-bottom: solid 1px #D1D1D1;
}
.form .row {
  width: 95%;
  float: right;
  height: 114rpx;
}
.form .row .icon {
  float: left;
  width: 60rpx;
  height: 60rpx;
  margin-top: 25rpx;
  margin-right: 20rpx;
}
.form .row label {
  line-height: 114rpx;
  float: left;
}
.form .row input {
  height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
  width: 450rpx;
}
.form .row .text-content {
  height: 114rpx;
  text-align: right;
  float: right;
  line-height: 114rpx;
  margin-right: 5%;
  max-width: 200rpx;
}
.form .row .select-icon {
  width: 30rpx;
  height: 30rpx;
  margin-top: 42rpx;
  margin-right: 5%;
  float: right;
}
.form .row .categoy-picker {
  height: 114rpx;
  line-height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row .time-picker {
  height: 114rpx;
  line-height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row .info-divider {
  text-align: right;
  float: right;
  height: 114rpx;
  line-height: 114rpx;
  width: 40rpx;
  text-align: center;
}
.form .row .selector {
  width: 30rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.form .row #announcement {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list {
  width: 100%;
  border-top: solid 1px #D1D1D1;
}
.list .row {
  width: 100%;
  float: right;
  height: 114rpx;
}
.list .row input {
  margin-left: 5%;
  height: 114rpx;
  text-align: left;
  float: left;
}
.list .row .delete-btn {
  width: 40rpx;
  height: 40rpx;
  margin-top: 37rpx;
  margin-right: 20rpx;
  float: right;
}
.list .row label {
  margin-left: 5%;
  line-height: 114rpx;
  float: left;
}
.list .row .selector {
  width: 100rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.logo {
  margin-top: 20rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 10px;
  border: 1px solid #E2E2E2;
}
.bottom-row {
  height: 90rpx;
  line-height: 90rpx;
  width: 100%;
  border-top: 1px solid #e2e2e2;
  position: absolute;
  bottom: 0;
  font-size: 30rpx;
}
.bottom-row .info {
  float: right;
  width: 35%;
  height: 100%;
  text-indent: 50rpx;
}
.bottom-row .btn {
  float: right;
  width: 30%;
  height: 100%;
  background-color: #FF4343;
  text-align: center;
  color: white;
}
.bottom-row .collect-button,
.bottom-row .home-button {
  width: 15%;
  height: 100%;
  float: left;
  border-left: 1px solid #e2e2e2;
}
.bottom-row .collect-button .top-wrapper,
.bottom-row .home-button .top-wrapper {
  width: 100%;
  height: 50%;
}
.bottom-row .collect-button .top-wrapper .star,
.bottom-row .home-button .top-wrapper .star {
  width: 35rpx;
  height: 35rpx;
  margin-top: 15rpx;
  margin-left: 40rpx;
  position: absolute;
}
.bottom-row .collect-button .buttom-wrapper,
.bottom-row .home-button .buttom-wrapper {
  text-align: center;
  font-size: 25rpx;
  line-height: 40rpx;
  width: 100%;
  height: 50%;
}
.bottom-row .collect-button {
  border-right: 1px solid #e2e2e2;
}
.std-button {
  width: 100%;
  margin-top: 30rpx;
}
.std-button .bind-user {
  background-color: #FF4343;
  color: white;
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
}
.selected-hover {
  background-color: #F0F0F0;
  width: 100%;
}
.banner {
  width: 100%;
  height: 200rpx;
}
.banner image {
  width: 100%;
  height: 100%;
}
.tag-highlight {
  width: 75rpx;
  float: left;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  color: white;
  border-radius: 5rpx;
  font-size: 20rpx;
}
.product-list-main .merchant-info {
  width: 100%;
  min-height: 220rpx;
}
.product-list-main .merchant-info__logo {
  margin-left: 15rpx;
  float: left;
  width: 220rpx;
  height: 100%;
  display: flex;
  justify-content: center;
}
.product-list-main .merchant-info__profile {
  margin-top: 23rpx;
  float: right;
  width: 500rpx;
  margin-right: 10rpx;
  height: 100%;
}
.product-list-main .merchant-info__name {
  font-size: 30rpx;
}
.product-list-main .merchant-info__address {
  margin-top: 10rpx;
  font-size: 23rpx;
  color: #979797;
}
.product-list-main .merchant-info__address .highlight {
  width: 75rpx;
  float: left;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  color: white;
  border-radius: 5rpx;
  font-size: 20rpx;
  background-color: #FFA32C;
}
.product-list-main .merchant-info__address text {
  margin-left: 10rpx;
  color: #FFA32C;
}
.product-list-main .merchant-info__hours {
  margin-top: 5rpx;
  font-size: 23rpx;
  color: #979797;
}
.product-list-main .merchant-info__announcement {
  margin-top: 10rpx;
  width: 450rpx;
  font-size: 23rpx;
  color: #979797;
  display: flex;
  word-wrap: break-word;
}
.product-list-main .merchant-info__announcement .highlight {
  width: 75rpx;
  float: left;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  color: white;
  border-radius: 5rpx;
  font-size: 20rpx;
  background-color: #FF4343;
}
.product-list-main .merchant-info__announcement text {
  color: #FF4343;
  margin-left: 10rpx;
  font-size: 24rpx;
  width: 350rpx;
}
.product-list-main .merchant-info__edit-profile-button,
.product-list-main .merchant-info__call-merchant-button {
  right: 0;
  top: 20rpx;
  position: absolute;
  height: 50rpx;
  font-size: 25rpx;
}
.product-list-main .merchant-info__edit-profile-button {
  width: 100rpx;
  color: #FF4343;
}
.product-list-main .merchant-info__edit-profile-button image {
  margin-left: 5rpx;
  margin-top: 5rpx;
  width: 30rpx;
  height: 30rpx;
}
.product-list-main .merchant-info__call-merchant-button {
  width: 150rpx;
  line-height: 50rpx;
  float: right;
}
.product-list-main .merchant-info__call-merchant-button image {
  margin-left: 5rpx;
  margin-top: 10rpx;
  width: 30rpx;
  height: 30rpx;
  position: absolute;
}
.product-list-main .products {
  height: 890rpx;
  width: 100%;
  overflow: scroll;
}
.product-list-main .products .categories {
  float: left;
  width: 180rpx;
  height: 890rpx;
  background-color: #F8F8F8;
  text-align: center;
  color: #999999;
  position: absolute;
  overflow-y: scroll;
}
.product-list-main .products .categories .category {
  height: 125rpx;
  line-height: 125rpx;
  width: 100%;
}
.product-list-main .products .categories .category--icon-style {
  width: 20%;
  height: 30%;
  float: left;
  margin-top: 40rpx;
  margin-left: 20rpx;
}
.product-list-main .products .categories .category--text-style {
  margin-left: -30rpx;
}
.product-list-main .products .categories .category--button-style {
  border-radius: 50%;
  background-color: #FF4343;
  height: 50rpx;
  width: 50rpx;
  margin-top: 36.5rpx;
  display: flex;
  align-items: center;
  float: left;
  margin-left: 20rpx;
}
.product-list-main .products .categories .category--button-style image {
  height: 40%;
  width: 40%;
  margin-left: 30%;
}
.product-list-main .products .categories .category--button-text {
  margin-left: -20rpx;
  color: #FF4343;
}
.product-list-main .products__empty-icon {
  position: absolute;
  height: 265rpx;
  width: 144rpx;
  margin-left: 390rpx;
  margin-top: 200rpx;
}
.product-list-main .products__empty-hint {
  position: absolute;
  height: 265rpx;
  width: 570rpx;
  margin-left: 180rpx;
  margin-top: 490rpx;
  color: #999999;
  text-align: center;
  font-size: 25rpx;
}
.product-list-main .products .product {
  margin-top: 20rpx;
  float: right;
  height: 180rpx;
  width: 73%;
}
.product-list-main .products .product__image {
  float: left;
  width: 180rpx;
  height: 180rpx;
}
.product-list-main .products .product__detail {
  width: 60%;
  float: right;
  margin-right: 20rpx;
}
.product-list-main .products .product__name {
  font-size: 35rpx;
}
.product-list-main .products .product__intro {
  height: 70rpx;
  margin-top: 5rpx;
  color: #888888;
  font-size: 24rpx;
  text-overflow: ellipsis;
  overflow: hidden;
}
.product-list-main .products .product__price {
  margin-top: 10rpx;
  color: #FF4343;
  font-size: 35rpx;
  font-weight: 500;
}
.product-list-main .products__add-button {
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  background: #FF4343;
  position: fixed;
  right: 0;
  bottom: 0;
  margin-right: 50rpx;
  margin-bottom: 50rpx;
}
.product-list-main .products__add-button image {
  height: 100%;
  width: 100%;
}

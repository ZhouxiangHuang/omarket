.margin-space-top {
  margin-top: 30rpx;
}
.underline {
  border-bottom: solid 1px #D1D1D1;
}
.underline-light {
  border-bottom: #E6E5F4;
}
.underline-light-gray {
  border-bottom: solid 1px #E2E2E2;
}
.border-right {
  border-right: solid 1px #D1D1D1;
}
.filter {
  background-color: white;
  width: 100%;
  height: 90rpx;
}
.filter .selection {
  margin-top: 15rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 49.6%;
  display: inline-block;
  text-align: center;
  color: #FF9E9E;
}
.filter .selection .selector {
  width: 20rpx;
  height: 15rpx;
  float: right;
  margin-right: 100rpx;
  margin-top: 25rpx;
}
.filter .selection text {
  margin-left: 100rpx;
}
.form {
  width: 100%;
  border-top: solid 1px #D1D1D1;
  border-bottom: solid 1px #D1D1D1;
}
.form .row {
  width: 95%;
  float: right;
  height: 114rpx;
}
.form .row .icon {
  float: left;
  width: 60rpx;
  height: 60rpx;
  margin-top: 25rpx;
  margin-right: 20rpx;
}
.form .row label {
  line-height: 114rpx;
  float: left;
}
.form .row input {
  height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
  width: 450rpx;
}
.form .row .text-content {
  height: 114rpx;
  text-align: right;
  float: right;
  line-height: 114rpx;
  margin-right: 5%;
  max-width: 200rpx;
}
.form .row .select-icon {
  width: 50rpx;
  height: 50rpx;
  margin-top: 32rpx;
  margin-right: 5%;
  float: right;
}
.form .row .categoy-picker {
  height: 114rpx;
  line-height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row .time-picker {
  height: 114rpx;
  line-height: 114rpx;
  text-align: right;
  float: right;
  margin-right: 5%;
}
.form .row .info-divider {
  text-align: right;
  float: right;
  height: 114rpx;
  line-height: 114rpx;
  width: 40rpx;
  text-align: center;
}
.form .row .selector {
  width: 30rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.form .row #announcement {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list {
  width: 100%;
  border-top: solid 1px #D1D1D1;
}
.list .row {
  width: 100%;
  float: right;
  height: 114rpx;
}
.list .row input {
  margin-left: 5%;
  height: 114rpx;
  text-align: left;
  float: left;
}
.list .row .delete-btn {
  width: 40rpx;
  height: 40rpx;
  margin-top: 37rpx;
  margin-right: 20rpx;
  float: right;
}
.list .row label {
  margin-left: 5%;
  line-height: 114rpx;
  float: left;
}
.list .row .selector {
  width: 100rpx;
  height: 100%;
  margin-right: 5%;
  line-height: 114rpx;
  float: right;
}
.logo {
  margin-top: 20rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 10px;
  border: 1px solid #E2E2E2;
}
.bottom-row {
  height: 90rpx;
  line-height: 90rpx;
  width: 100%;
  border-top: 1px solid #e2e2e2;
  position: absolute;
  bottom: 0;
  font-size: 30rpx;
}
.bottom-row .info {
  float: right;
  width: 35%;
  height: 100%;
  text-indent: 50rpx;
}
.bottom-row .btn {
  float: right;
  width: 30%;
  height: 100%;
  background-color: #FF4343;
  text-align: center;
  color: white;
}
.bottom-row .collect-button,
.bottom-row .home-button {
  width: 15%;
  height: 100%;
  float: left;
  border-left: 1px solid #e2e2e2;
}
.bottom-row .collect-button .top-wrapper,
.bottom-row .home-button .top-wrapper {
  width: 100%;
  height: 50%;
}
.bottom-row .collect-button .top-wrapper .star,
.bottom-row .home-button .top-wrapper .star {
  width: 35rpx;
  height: 35rpx;
  margin-top: 15rpx;
  margin-left: 40rpx;
  position: absolute;
}
.bottom-row .collect-button .buttom-wrapper,
.bottom-row .home-button .buttom-wrapper {
  text-align: center;
  font-size: 25rpx;
  line-height: 40rpx;
  width: 100%;
  height: 50%;
}
.bottom-row .collect-button {
  border-right: 1px solid #e2e2e2;
}
.std-button {
  width: 100%;
  margin-top: 30rpx;
}
.std-button .bind-user {
  background-color: #FF4343;
  color: white;
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
}
.selected-hover {
  background-color: #F0F0F0;
  width: 100%;
}
.banner {
  width: 100%;
  height: 200rpx;
}
.banner image {
  width: 100%;
  height: 100%;
}
.edit-merch-main .top-logo {
  height: 220rpx;
  width: 100%;
  display: flex;
  justify-content: center;
}
.edit-merch-main .top-logo .logo {
  background-color: #FF9E9E;
}
.edit-merch-main .top-logo .logo .add {
  width: 70rpx;
  height: 70rpx;
  margin-top: 55rpx;
  margin-left: 55rpx;
}
.edit-merch-main .form {
  margin-top: 40rpx;
}
.edit-merch-main .form .currency picker {
  width: 450rpx;
}
.edit-merch-main .form .row .text-content {
  width: 450rpx;
}
.edit-merch-main .std-tags {
  height: 100%;
  display: flex;
  align-items: center;
  float: right;
  margin-right: 5%;
}
.edit-merch-main .std-tags .tag {
  margin-right: 10rpx;
  float: right;
  border: 1px solid #FFA32C;
  text-align: center;
  border-radius: 3px;
  height: 30rpx;
  line-height: 30rpx;
  min-width: 110rpx;
  font-size: 24rpx;
}
.edit-merch-main .std-tags .tag .tag-text {
  color: #FFA32C;
}
